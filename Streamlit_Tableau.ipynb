        "            'Segmento': [encoded_options['Segmento'][segmento]],\n",
        "            'Cantidad': [cantidad],\n",
        "            'Descuento': [descuento]\n",
        "        })\n",
        "\n",
        "        pred = model.predict(input_data)[0]\n",
        "        st.metric(label=\"💰 Predicción de ventas\", value=f\"${pred:,.2f}\")\n",
